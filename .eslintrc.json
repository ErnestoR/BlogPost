{
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "trailingComma": "all", "printWidth": 120 }
    ]
  }
}
